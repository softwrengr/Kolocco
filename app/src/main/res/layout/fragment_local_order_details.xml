                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/dp_3">

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextAccept"
                    style="@style/home_icon_title"
                    android:layout_width="0dp"
                    android:layout_gravity="bottom"
                    android:layout_weight="1"
                    android:drawableTop="@drawable/pending_ord_detail_accept"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:text="@string/notification_accept"
                    android:textColor="@color/green"
                    app:fontFamily="@font/opensans_regular" />


                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextDecline"
                    style="@style/home_icon_title"
                    android:layout_width="0dp"
                    android:layout_gravity="bottom"
                    android:layout_weight="1"
                    android:drawableTop="@drawable/pending_ord_detail_decline"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:text="@string/notification_decline"
                    android:textColor="@color/red"
                    app:fontFamily="@font/opensans_regular" />


                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextModify"
                    style="@style/home_icon_title"
                    android:layout_gravity="bottom"
                    android:layout_weight="1"
                    android:drawableTop="@drawable/pending_ord_detail_modify"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:text="@string/modify"
                    android:textColor="@color/buttonColor"
                    android:visibility="gone"
                    app:fontFamily="@font/opensans_regular" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayoutAcceptButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/dp_3"
                android:visibility="gone">

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextChat"
                    style="@style/home_icon_title"
                    android:layout_width="0dp"
                    android:layout_gravity="bottom"
                    android:layout_weight="1"
                    android:drawableTop="@drawable/accept_ord_detail_chat"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:text="@string/notification_chat"
                    android:textColor="@color/yellow"
                    app:fontFamily="@font/opensans_regular" />


                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextComplete"
                    style="@style/home_icon_title"
                    android:layout_width="0dp"
                    android:layout_gravity="bottom"
                    android:layout_weight="1"
                    android:drawableTop="@drawable/accept_ord_detail_complete"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:text="@string/complete"
                    android:textColor="@color/green"
                    app:fontFamily="@font/opensans_regular" />


                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextCancel"
                    style="@style/home_icon_title"
                    android:layout_width="0dp"
                    android:layout_gravity="bottom"
                    android:layout_weight="1"
                    android:drawableTop="@drawable/accept_ord_detail_cancel"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:text="@string/cancel"
                    android:textColor="@color/red"
                    app:fontFamily="@font/opensans_regular" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayoutComplateButton"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/dp_3"
                android:visibility="gone">

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextChatNew"
                    style="@style/home_icon_title"
                    android:layout_width="0dp"
                    android:layout_gravity="bottom"
                    android:layout_weight="1"
                    android:drawableTop="@drawable/accept_ord_detail_chat"
                    android:gravity="center"
                    android:includeFontPadding="false"
                    android:text="@string/notification_chat"
                    android:textColor="@color/yellow"

                    app:fontFamily="@font/opensans_regular" />


                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"

                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageViewSendPayment"
                        android:layout_width="@dimen/dp_45"
                        android:layout_height="@dimen/dp_35"
                        android:layout_gravity="center"
                        android:layout_marginBottom="@dimen/dp_7"
                        android:src="@drawable/accept_ord_popup_payment_request_b" />

                    <android.support.v7.widget.AppCompatTextView
                        android:id="@+id/customTextSendPaymentReq"
                        style="@style/home_icon_title"
                        android:layout_gravity="center"
                        android:gravity="center|bottom"
                        android:includeFontPadding="false"
                        android:text="@string/send_payment_req_new"
                        android:textColor="@color/buttonColor"
                        app:fontFamily="@font/opensans_regular" />

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayoutButtonRat"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">


                <android.support.v7.widget.AppCompatButton
                    android:id="@+id/buttonGiveRate"
                    style="@style/button"
                    android:layout_width="@dimen/dp_165"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/dp_10"
                    android:text="@string/button_give_rate" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_25" />

        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
