<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linearLayoutRow"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/customTextViewName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:clickable="false"
            android:drawablePadding="@dimen/dp_10"
            android:includeFontPadding="false"
            android:paddingBottom="@dimen/dp_14"
            android:paddingTop="@dimen/dp_14"
            android:text="Session"
            android:textColor="@color/greyText"
            android:textSize="@dimen/sp_11"
            app:fontFamily="@font/opensans_regular" />

        <android.support.v7.widget.AppCompatImageView
            android:id="@+id/imageViewIndicator"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:clickable="false"
            android:paddingBottom="@dimen/dp_5"
            android:paddingStart="@dimen/dp_5"
            android:paddingTop="@dimen/dp_5"
            android:src="@drawable/month_wise_earning_arrow" />

        <android.support.v7.widget.AppCompatImageView
            android:id="@+id/imageViewIndicatorYes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:clickable="false"
            android:paddingBottom="@dimen/dp_5"
            android:paddingStart="@dimen/dp_5"
            android:paddingTop="@dimen/dp_5"
            android:src="@drawable/appoinment_booked_tick_sign" />

    </LinearLayout>

    <View style="@style/bottom_line_s_m" />

</LinearLayout>