<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/drawable_round_corner_small_white"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/linearLayoutContent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/dp_6"
        android:paddingEnd="@dimen/dp_6"
        android:paddingStart="@dimen/dp_6"
        android:paddingTop="@dimen/dp_6">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageViewProfile"
                android:layout_width="@dimen/dp_50"
                android:layout_height="@dimen/dp_50"
                android:layout_gravity="start"
                android:paddingBottom="@dimen/dp_5"
                android:paddingEnd="@dimen/dp_5"
                android:src="@drawable/user_round" />

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/customTextCompleted"
                style="@style/home_icon_title"
                android:drawablePadding="@dimen/dp_2"
                android:drawableTop="@drawable/complete_order_icon"
                android:gravity="start|bottom"
                android:includeFontPadding="false"
                android:paddingEnd="@dimen/dp_2"
                android:paddingTop="@dimen/dp_2"
                android:text="@string/completed"
                android:textColor="@color/green"
                android:visibility="visible"
                app:fontFamily="@font/opensans_regular" />

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/customTextStatus"
                style="@style/home_icon_title"
                android:gravity="start|bottom"
                android:includeFontPadding="false"
                android:paddingEnd="@dimen/dp_2"
                android:paddingTop="@dimen/dp_2"
                android:text="@string/completed"
                android:textColor="@color/yellow"
                android:visibility="gone"
                app:fontFamily="@font/opensans_regular" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/mdp_4"
            android:layout_marginStart="@dimen/dp_2"
            android:layout_weight="1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextViewName"
                    style="@style/visitor_dashboard_review_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingTop="0dp"
                    tools:text="Backend Data"
                    android:layout_gravity="center"
                    android:textColor="@color/black"
                    android:textSize="@dimen/sp_12"
                    app:fontFamily="@font/opensans_regular_title" />


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <android.support.v7.widget.AppCompatTextView
                        android:id="@+id/textViewCurrencyType"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_marginEnd="@dimen/dp_3"
                        android:ellipsize="end"
                        android:includeFontPadding="false"
                        android:maxLines="1"
                        android:textColor="@color/greyText"
                        android:textSize="@dimen/sp_9"
                        app:fontFamily="@font/opensans_regular_title"
                        tools:text="@string/usd" />

                    <android.support.v7.widget.AppCompatTextView
                        android:id="@+id/customTextViewPrice"
                        style="@style/order_history_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="0dp"
                        android:layout_marginEnd="0dp"
                        android:includeFontPadding="false"
                        android:textColor="@color/buttonColor"
                        android:textSize="@dimen/sp_11"
                        app:fontFamily="@font/opensans_regular_title"
                        tools:text="25.25" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextViewServiceType"
                    style="@style/order_history_text_new"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/greyText"
                    app:fontFamily="@font/opensans_regular"
                    tools:text="Surfing with demo test demo this is for sdahjaj daj ajdhjahsdjh ajd" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextViewServiceName"
                    style="@style/order_history_text_new"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/greyText"
                    app:fontFamily="@font/opensans_regular"
                    tools:text="Sport test" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextViewAddress"
                    style="@style/order_history_text_new"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingBottom="0dp"
                    android:textColor="@color/greyText"
                    app:fontFamily="@font/opensans_regular"
                    tools:text="Ahmedabad,India" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    style="@style/order_history_text_new"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="0dp"
                    android:text="@string/local"
                    android:textColor="@color/greyText"
                    app:fontFamily="@font/opensans_regular" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextViewLocalName"
                    style="@style/order_history_text_new"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ellipsize="end"
                    android:lines="1"
                    android:paddingBottom="0dp"
                    android:text=""
                    android:textColor="@color/buttonColor"
                    app:fontFamily="@font/opensans_regular" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextViewPaymentRule"
                    style="@style/order_history_text_new"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:ellipsize="end"
                    android:lines="1"
                    android:paddingBottom="@dimen/dp_2"
                    android:text=""
                    android:textColor="@color/greyText"
                    app:fontFamily="@font/opensans_regular" />

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

</LinearLayout>