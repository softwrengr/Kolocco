<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/toolbar_trans" />

    <android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            android:orientation="vertical">


            <LinearLayout style="@style/dashboard_main_space">

                <android.support.v7.widget.AppCompatTextView
                    style="@style/visitor_dashboard_details_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:includeFontPadding="false"
                    android:text="@string/appointment_summery_experience_new"
                    app:fontFamily="@font/opensans_regular_title" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/dp_4"
                android:orientation="vertical">

                <include layout="@layout/exp_local_row_layout" />

            </LinearLayout>

            <LinearLayout style="@style/dashboard_main_space">

                <View style="@style/bottom_line" />

            </LinearLayout>


            <LinearLayout style="@style/dashboard_main_space">


                <!--Date and Time section end -->

                <!--Date and Time section start -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <android.support.v7.widget.AppCompatTextView
                            style="@style/visitor_dashboard_details_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/order_detail_date"
                            app:fontFamily="@font/opensans_regular_title" />

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewDate"
                            style="@style/visitor_dashboard_details_text_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:fontFamily="@font/opensans_regular" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/textViewTime"
                            style="@style/visitor_dashboard_details_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/order_detail_time"
                            app:fontFamily="@font/opensans_regular_title" />

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewTime"
                            style="@style/visitor_dashboard_details_text_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:fontFamily="@font/opensans_regular" />

                    </LinearLayout>

                </LinearLayout>

                <View style="@style/bottom_line" />

                <!--Date and Time section end -->

                <!--Duration and participants section start -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <android.support.v7.widget.AppCompatTextView
                            style="@style/visitor_dashboard_details_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/order_detail_duration"
                            app:fontFamily="@font/opensans_regular_title" />

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewDuration"
                            style="@style/visitor_dashboard_details_text_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:fontFamily="@font/opensans_regular" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <android.support.v7.widget.AppCompatTextView
                            style="@style/visitor_dashboard_details_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/order_detail_participants"
                            app:fontFamily="@font/opensans_regular_title" />

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewParticipants"
                            style="@style/visitor_dashboard_details_text_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:fontFamily="@font/opensans_regular" />

                    </LinearLayout>

                </LinearLayout>

                <View style="@style/bottom_line" />

                <!--Duration and participants section end -->

                <!--Meeting Spot section start -->


                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center|start"
                        android:orientation="vertical">

                        <android.support.v7.widget.AppCompatTextView
                            style="@style/visitor_dashboard_details_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/order_detail_meeting_spot"
                            app:fontFamily="@font/opensans_regular_title" />


                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewLocationMeeting"
                            style="@style/visitor_dashboard_details_text_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=""
                            app:fontFamily="@font/opensans_regular" />

                    </LinearLayout>

                    <FrameLayout
                        android:id="@+id/frameLayoutMap"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end|center"
                        android:visibility="gone">


                        <ImageView
                            android:layout_width="@dimen/dp_105"
                            android:layout_height="@dimen/dp_55"
                            android:layout_gravity="end|center"
                            android:scaleType="centerCrop"
                            android:src="@drawable/staticmap" />

                        <ImageView
                            android:layout_width="@dimen/dp_75"
                            android:layout_height="@dimen/dp_55"
                            android:background="@drawable/drawable_white_gradint" />

                    </FrameLayout>

                </FrameLayout>

                <View style="@style/bottom_line" />

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/textView"
                    style="@style/visitor_dashboard_details_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:includeFontPadding="false"
                    android:text="@string/activity_duration_summery_new"
                    android:textColor="@color/black"
                    app:fontFamily="@font/opensans_regular_title" />


                <android.support.v7.widget.RecyclerView
                    android:id="@+id/recyclerAddParticipants"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="visible"
                    tools:listitem="@layout/row_add_participants_with_price" />

                <!--Receipt total activity section start -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <android.support.v7.widget.AppCompatTextView
                            style="@style/receipt_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/appointment_summery_total_activity"
                            android:textColor="@color/buttonColor"
                            app:fontFamily="@font/opensans_regular" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewRecTotalAct"
                            style="@style/receipt_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="$0"
                            android:textColor="@color/buttonColor"
                            app:fontFamily="@font/opensans_regular" />

                    </LinearLayout>

                </LinearLayout>

                <!--Receipt total activity section section end -->

                <!--Receipt Fees for booking section start -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <android.support.v7.widget.AppCompatTextView
                            style="@style/receipt_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/booking_fee"
                            app:fontFamily="@font/opensans_regular_title" />


                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewRecFeesFBookingPrice"
                            style="@style/receipt_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="$00"
                            app:fontFamily="@font/opensans_regular_title" />

                    </LinearLayout>

                </LinearLayout>

                <!-- Receipt Fees for booking section end -->

                <!--Referral section section end -->

                <LinearLayout
                    android:id="@+id/linearLayoutReferralAmount"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <android.support.v7.widget.AppCompatTextView
                            style="@style/receipt_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="@string/referral_amount"
                            android:textColor="@color/red"
                            app:fontFamily="@font/opensans_regular_title" />

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewReferralValue"
                            style="@style/receipt_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:includeFontPadding="false"
                            android:text="$0"
                            android:textColor="@color/red"
                            app:fontFamily="@font/opensans_regular_title" />

                    </LinearLayout>

                </LinearLayout>

                <!--Referral booking section end -->

                <View style="@style/bottom_line" />

                <!--Receipt total booking section start -->

                <android.support.v7.widget.AppCompatTextView
                    android:id="@+id/customTextViewRecTotalBooking"
                    style="@style/receipt_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:includeFontPadding="false"
                    android:text="$0"
                    android:textColor="@color/buttonColor"
                    android:textSize="@dimen/sp_14"
                    app:fontFamily="@font/opensans_regular_title" />

                <android.support.v7.widget.AppCompatTextView
                    style="@style/receipt_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:text="@string/appointment_summery_total_booking"
                    android:textColor="@color/buttonColor"
                    app:fontFamily="@font/opensans_regular" />

                <!--Receipt total booking section section end -->

                <View style="@style/bottom_line" />

                <!--Receipt section end -->


                <!--Raise Objection section start-->

                <LinearLayout
                    android:id="@+id/linearLayoutRaiseObj"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <android.support.v7.widget.AppCompatCheckBox
                        android:id="@+id/checkboxChoose"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="start"
                        android:button="@null"
                        android:checked="false"
                        android:clickable="false"
                        android:drawableLeft="@drawable/selector_checkbox"
                        android:textColor="@color/greyText"
                        android:textSize="@dimen/sp_10"
                        app:fontFamily="@font/opensans_regular" />

                    <android.support.v7.widget.AppCompatTextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_weight="1"
                        android:clickable="false"
                        android:text="@string/receipt_raise_objection"
                        android:textColor="@color/greyText"
                        android:textSize="@dimen/sp_11"
                        app:fontFamily="@font/opensans_regular" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearLayoutRaiseObjection"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_5"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/linearLayoutRefundTotal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/dp_5"
                        android:paddingTop="@dimen/dp_5">

                        <RadioButton
                            android:id="@+id/radioButtonRaiseTotal"
                            style="@style/raise_obj_radio"
                            android:layout_height="match_parent"
                            android:gravity="center|start" />

                        <android.support.v7.widget.AppCompatTextView
                            style="@style/raise_obj_radio_text"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center|start"
                            android:text="@string/receipt_do_you_want_total_refund"
                            app:fontFamily="@font/opensans_regular" />

                        <android.support.v7.widget.AppCompatEditText
                            style="@style/raise_obj_edit_text"
                            android:layout_gravity="center"
                            android:hint="00%"
                            android:inputType="numberDecimal"
                            android:nextFocusDown="@+id/customTexteditTextWriteNow"
                            android:visibility="invisible"
                            app:fontFamily="@font/opensans_regular" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayoutRefundPercentage"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/dp_5"
                        android:paddingTop="@dimen/dp_5">

                        <RadioButton
                            android:id="@+id/radioButtonRaisePersentage"
                            style="@style/raise_obj_radio"
                            android:layout_height="match_parent"
                            android:gravity="center|start" />

                        <android.support.v7.widget.AppCompatTextView
                            style="@style/raise_obj_radio_text"
                            android:layout_height="wrap_content"
                            android:gravity="center|start"
                            android:text="@string/receipt_do_you_want_percentage_discount_new"
                            app:fontFamily="@font/opensans_regular" />

                        <android.support.v7.widget.AppCompatEditText
                            android:id="@+id/customEditTextPersntageDiscount"
                            style="@style/raise_obj_edit_text"
                            android:layout_gravity="center"
                            android:hint="00%"
                            android:inputType="numberDecimal"
                            android:nextFocusDown="@+id/customTexteditTextWriteNow"
                            app:fontFamily="@font/opensans_regular" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/linearLayoutRefundFlatAmount"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingBottom="@dimen/dp_5"
                        android:paddingTop="@dimen/dp_5">

                        <RadioButton
                            android:id="@+id/radioButtonFlatAmount"
                            style="@style/raise_obj_radio"
                            android:layout_height="match_parent"
                            android:gravity="center|start" />

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customEdiTextFlatAmountText"
                            style="@style/raise_obj_radio_text"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center|start"
                            android:text="@string/receipt_do_you_want_flat_amount_discount_new"
                            app:fontFamily="@font/opensans_regular" />

                        <android.support.v7.widget.AppCompatEditText
                            android:id="@+id/customEdiTextFlatAmount"
                            style="@style/raise_obj_edit_text"
                            android:layout_gravity="center"
                            android:hint="@string/_000"
                            android:inputType="numberDecimal"
                            android:nextFocusDown="@+id/customTexteditTextWriteNow"
                            app:fontFamily="@font/opensans_regular" />

                    </LinearLayout>

                    <com.kooloco.util.FirstCapEditText
                        android:id="@+id/customTexteditTextWriteNow"
                        style="@style/visitor_dashboard_details_text_value"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/dp_5"
                        android:background="@drawable/drawable_round_border_grey"
                        android:gravity="top"
                        android:hint="@string/receipt_write_your_reason"
                        android:lines="4"
                        android:padding="@dimen/dp_12"
                        android:textColor="@color/blackTextColor"
                        android:textColorHint="@color/hintTextColor"
                        android:textSize="@dimen/sp_12"
                        app:fontFamily="@font/opensans_regular" />

                </LinearLayout>

                <!--Raise Objection section end-->

                <LinearLayout style="@style/button_next">


                    <android.support.v7.widget.AppCompatButton
                        android:id="@+id/buttonSendObjection"
                        style="@style/button"
                        android:layout_width="match_parent"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:text="@string/button_pay_now" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_25" />
            </LinearLayout>

        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
</LinearLayout>

