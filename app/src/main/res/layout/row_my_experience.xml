<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="@dimen/dp_15"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <View
                android:id="@+id/viewTop"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:background="@color/buttonColor" />

            <ImageView
                android:id="@+id/imageViewLive"
                android:layout_width="@dimen/dp_5"
                android:layout_height="@dimen/dp_5"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/dp_3"
                android:layout_marginTop="@dimen/dp_3"
                android:src="@drawable/drawable_recent_chat_blue" />

            <View
                android:id="@+id/viewBottom"
                android:layout_width="1dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:background="@color/buttonColor" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageViewProfile"
                    android:layout_width="@dimen/dp_35"
                    android:layout_height="@dimen/dp_35"
                    android:padding="@dimen/dp_5"
                    android:src="@drawable/user_round" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <android.support.v7.widget.AppCompatTextView
                        android:id="@+id/customTextViewLocalName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:includeFontPadding="false"
                        android:text="Doris Candiz"
                        android:textColor="@color/black"
                        android:textSize="@dimen/sp_10"
                        android:singleLine="true"
                        android:ellipsize="end"
                        app:fontFamily="@font/opensans_regular_title" />

                    <android.support.v7.widget.AppCompatTextView
                        android:id="@+id/customTextViewServiceTypeValue"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="10 June, 2017"
                        android:textColor="@color/greyText"
                        android:textSize="@dimen/sp_9"
                        app:fontFamily="@font/opensans_regular" />

                </LinearLayout>


            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:foreground="@drawable/ripple_animation"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="@dimen/dp_15"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="horizontal">

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:background="@color/buttonColor" />

        </LinearLayout>

        <LinearLayout xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/linearLayoutExpRoot"
            style="@style/exp_home_style"
            android:layout_weight="1"
            android:clickable="true">

            <com.github.siyamed.shapeimageview.mask.PorterShapeImageView
                android:id="@+id/imageView"
                style="@style/exp_style_image"
                android:src="@drawable/place"
                app:siShape="@drawable/shape_rounded_rectangle_exp"
                tools:src="@drawable/temp_2" />

            <LinearLayout
                android:id="@+id/linearLayoutMain"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/dp_5"
                android:layout_marginVertical="@dimen/dp_1"

                android:clickable="true"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewTitle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="start"
                            android:ellipsize="end"
                            android:includeFontPadding="false"
                            android:lines="2"
                            android:maxLines="2"
                            android:padding="@dimen/dp_3"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="@dimen/sp_13"
                            app:fontFamily="@font/opensans_regular_title"
                            tools:text="Surf Lesson in the Boro with lunch &amp; all other services" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:paddingHorizontal="@dimen/dp_3">

                            <android.support.v7.widget.AppCompatTextView
                                android:id="@+id/textViewCurrencyType"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="bottom"
                                android:ellipsize="end"
                                android:includeFontPadding="false"
                                android:maxLines="3"
                                android:paddingBottom="@dimen/dp_3"
                                android:text="@string/total_of_usd"
                                android:textColor="@color/greyText"
                                android:textSize="@dimen/sp_11"
                                app:fontFamily="@font/opensans_regular_title" />

                            <android.support.v7.widget.AppCompatTextView
                                android:id="@+id/textViewExpPrice"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="start"
                                android:ellipsize="end"
                                android:includeFontPadding="false"
                                android:maxLines="3"
                                android:padding="@dimen/dp_3"
                                android:text="00.00"
                                android:textColor="@color/buttonColor"
                                android:textSize="@dimen/sp_12"
                                app:fontFamily="@font/opensans_regular_title"
                                tools:text="20.20" />

                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingEnd="@dimen/dp_3"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/imageViewExp"
                            android:layout_width="@dimen/dp_45"
                            android:layout_height="@dimen/dp_53"
                            android:layout_gravity="center|end"
                            android:src="@drawable/place"
                            android:visibility="visible"
                            tools:src="@drawable/lesson_new" />


                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:layout_weight="1">

                        <android.support.v7.widget.AppCompatRatingBar
                            android:id="@+id/ratingBar"
                            style="@style/Base.Widget.AppCompat.RatingBar.Small"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/dp_3"
                            android:layout_marginEnd="@dimen/dp_3"
                            android:layout_marginTop="@dimen/dp_3"
                            android:progress="4"
                            android:rating="4.5"
                            android:theme="@style/RatingBar" />

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/textViewReviewCount"
                            style="@style/order_history_text_new"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="0dp"
                            android:layout_weight="1"
                            android:textColor="@color/greyText"
                            app:fontFamily="@font/opensans_regular"
                            tools:text="(5)" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">


                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewParticipants"
                            style="@style/my_experince_text"
                            android:layout_width="match_parent"
                            android:layout_gravity="end"
                            android:gravity="end"
                            android:textColor="@color/greyText"
                            app:fontFamily="@font/opensans_regular"
                            tools:text=" 1 Participants" />

                    </LinearLayout>
                </LinearLayout>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:clickable="false"
                    android:orientation="vertical"
                    android:paddingHorizontal="@dimen/dp_3">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"

                            android:orientation="horizontal">

                            <android.support.v7.widget.AppCompatTextView
                                style="@style/my_experince_text"
                                android:layout_gravity="start"
                                android:text="@string/my_exp_duration"
                                android:textColor="@color/black"
                                app:fontFamily="@font/opensans_regular" />

                            <android.support.v7.widget.AppCompatTextView
                                android:id="@+id/customTextViewDuration"
                                style="@style/my_experince_text"
                                android:layout_gravity="start"
                                android:textColor="@color/greyText"
                                app:fontFamily="@font/opensans_regular"
                                tools:text=" 3 hours" />


                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <android.support.v7.widget.AppCompatTextView
                            style="@style/my_experince_text"
                            android:layout_gravity="start"
                            android:text="@string/my_exp_meeting_spot"
                            android:textColor="@color/black"
                            android:visibility="gone"
                            app:fontFamily="@font/opensans_regular" />

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/customTextViewLocation"
                            style="@style/my_experince_text"
                            android:layout_width="match_parent"
                            android:layout_weight="1"
                            android:layout_gravity="start"
                            tools:text="3668 Hood Avenue San, CA 92103"
                            android:textColor="@color/greyText"
                            android:maxLines="1"
                            android:ellipsize="end"
                            android:drawableLeft="@drawable/map_pin_home"
                            android:drawablePadding="@dimen/dp_3"
                            app:fontFamily="@font/opensans_regular" />

                        <android.support.v7.widget.AppCompatTextView
                            android:id="@+id/textViewStatus"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom"
                            android:ellipsize="end"
                            android:includeFontPadding="false"
                            android:maxLines="1"
                            android:paddingVertical="@dimen/dp_3"
                            android:paddingStart="@dimen/dp_6"
                            android:textColor="@color/greyText"
                            android:textSize="@dimen/sp_11"
                            app:fontFamily="@font/opensans_regular_title"
                            tools:text="Pending" />

                    </LinearLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_3" />

                </LinearLayout>

            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

</LinearLayout>
